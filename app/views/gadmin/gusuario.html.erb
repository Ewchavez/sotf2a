                  <!-- <% cursos = Curso.all.map{|x| [x.nombre.to_s.capitalize] + [x.id.to_s+'_'+x.nombre.to_s]} %>-->

                  <% carrera = @Carrera.all.map{|x| [x.nombre.to_s.capitalize] + [x.id.to_s+'_'+x.nombre.to_s]} %>

                  <%= select_tag("selectedCarrera", options_for_select(carrera)) %>




                </td>

                <td>
                  <% cursos = @Curso.all.map{|x| [x.nombre.to_s.capitalize] + [x.id.to_s+'_'+x.nombre.to_s]} %>

                  <%= select_tag("selectedCurso", options_for_select(cursos)) %>






                  <button type="submit" class="btn btn-primary btn-xs">Asignar curso</button>


                </td>

              </form><!-- form -->




            </tbody>
          </table>

          <table class="table table-striped">

            <thead>
              <tr>
                <th> Curso</th>
                <th> Semestre</th>
                <th> Sección</th>


              </tr>
            </thead>

            <form method="get" action="enlazar">

              <td>
                <% cursos = @Curso.all.map{|x| [x.nombre.to_s.capitalize] + [x.id.to_s+'_'+x.nombre.to_s]} %>

                <%= select_tag("selectedCurso", options_for_select(cursos)) %>


              </td>

              <td>
                <% semes = @Semestre.all.map{|x| [x.fecha.to_s.capitalize] + [x.id.to_s+'_'+x.fecha.to_s]} %>

                <%= select_tag("selectedseme", options_for_select(semes)) %>


              </td>
              <td>
                <% secci = @Seccion.all.map{|x| [x.idsec.to_s.capitalize] + [x.id.to_s+'_'+x.idsec.to_s]} %>

                <%= select_tag("selectedsecc", options_for_select(secci)) %>

                <button type="submit" class="btn btn-primary btn-xs">Asignar curso</button>

              </td>



            </form><!-- form -->




          </tbody>
        </table>





        <table class="table table-striped">

