                      <%= select_tag("selectedprofe", options_for_select(carrera)) %>



                    </td>
                    <td>
                      <% secci = @SeccionD.all.map{|x| [ x.idsec.to_s + x.nombre.to_s ] + [x.id.to_s+'_'+x.idsec.to_s]} %>

                      <%= select_tag("selectedsecc", options_for_select(secci)) %>

                      <button class="btn btn-primary btn-xs" type="submit" name="button">Asignar profesor</button>

                    </form>

                  </td>

                </form><!-- form -->


              </tbody>
            </table>


            <table class="table table-striped">

