  var clic =  1;
  var clic2 = 1;

  function cerrar() {
document.getElementById("log1").style.display="none";
 document.getElementById("regH").style.display="none";

  }
  function ingresar(){

  if(clic==1){
     document.getElementById("log1").style.display="block";
     clic = clic + 1;
     } else{
         document.getElementById("log1").style.display="none";
      clic = 1;
     }

  }

  function  registraru(){
         document.getElementById("log1").style.display="none";

  	if(clic2==1){
  	   document.getElementById("regH").style.display="block";
  	   clic2 = clic2 + 1;
  	   } else{
  	       document.getElementById("regH").style.display="none";
  	    clic2 = 1;
  	   }
  }

  function finalR(){
  document.getElementById("regH").style.display="none";
   document.getElementById("log1").style.display="none";

  }

  </script>



